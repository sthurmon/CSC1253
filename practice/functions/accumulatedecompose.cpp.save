#include <iostream>
using namespace std;

void accumulate(int a[], int n){
for(int i=0; i<n; i++) a[i]+=a[i-1];
}
void decompose(int a[], int n){
for(int i=0; i<n; i++) a[i]=a[i]-a[i-1];
}
void print(int a[], int n){
for(int i=0; i<n; i++) cout << a[i] << " ";
}
int main(){
int n=9; int a[]={1,2,1,2,3,1,2,1,2}; int b[n];
print(a, n)\n;
accumulate(a, n);
print(a, n)\n;
decompose(a, n);
print(a, n)\n;
}
